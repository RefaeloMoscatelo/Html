<h2>Mytime</h2>

<table>
    <thead>
        <tr>
            <th>Name</th>
            <th>Extension</th>
            <th>Size</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in files)
        {
            <tr>
           <td>@item.FileName</td>
           <td>@item.Extension</td>
           <td>@item.Size</td>
            </tr>
        }

    </tbody>
    </table>


